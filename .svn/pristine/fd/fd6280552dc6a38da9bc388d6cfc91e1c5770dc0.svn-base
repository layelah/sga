/******** Medias ******/
@media only screen and (max-width: 1150px) {
	.row .title-1 {
		font-size: 12px;
	}
	.row .title-2 {
		font-size: 18px;
	}
}

@media only screen and (max-width: 950px) {
	.row .title-1 {
		font-size: 10px;
	}
	.row .title-2 {
		font-size: 16px;
	}
}

@media only screen and (max-width: 750px) {
	.row .title-1 {
		font-size: 8px;
	}
	.row .title-2 {
		font-size: 14px;
	}
	.logo-sn img, .logo-tresor img {
		width: 70px;
	}
}

@media only screen and (max-width: 650px) {
	.row .title-1 {
		font-size: 8px;
	}
	.row .title-2 {
		font-size: 10px;
	}
	.row .title-2 {
		font-size: 14px;
	}
	.logo-sn img, .logo-tresor img {
		width: 40px;
	}
	
	.valid-btn {
		margin-top: 20px
	}
	
	.cancel-btn {
		margin-left : 0px;
	}
}

