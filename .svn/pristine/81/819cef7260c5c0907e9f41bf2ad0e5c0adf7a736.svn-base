<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html   xmlns="http://www.w3.org/1999/xhtml"
        xmlns:composite="http://java.sun.com/jsf/composite"
        xmlns:infina="http://java.sun.com/jsf/composite/infina">

    <composite:interface>
        <composite:attribute name="obj" required="true"/>
        <composite:attribute name="lbl_lib" default="#{msg.LBL_LIB}"/>
    </composite:interface>

    <composite:implementation>
        <infina:infoString titre="#{msg.LBL_CODE}" valeur="#{cc.attrs.obj.code}"/>
        <infina:infoString titre="#{cc.attrs.lbl_lib}" valeur="#{cc.attrs.obj.lib}"/>
        <infina:infoZoneText  titre="#{msg.LBL_DESCRIPTION}" valeur="#{cc.attrs.obj.description}" />
    </composite:implementation>
</html>