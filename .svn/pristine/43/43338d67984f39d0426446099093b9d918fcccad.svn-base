<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" 
      xmlns:c="http://java.sun.com/jsp/jstl/core"
      xmlns:composite="http://java.sun.com/jsf/composite" xmlns:h="http://xmlns.jcp.org/jsf/html" xmlns:f="http://java.sun.com/jsf/core" xmlns:infina="http://java.sun.com/jsf/composite/infina">

    <composite:interface>
        <composite:attribute name="obj" required="true"/>
    </composite:interface>

    <composite:implementation> 
        <c:if test="#{empty cc.attrs.obj}">
            <h:outputLabel value="SYSTEM" />           
        </c:if>
        <c:if test="#{not empty cc.attrs.obj}">
            <h:outputLabel value="#{cc.attrs.obj.nomComplet}" />           
        </c:if>        
    </composite:implementation>
</html>