<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:composite="http://java.sun.com/jsf/composite"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:b="http://bootsfaces.net/ui">

    <composite:interface>
        <composite:attribute name="bean" required="true"/>
        <composite:attribute name="message" required="true"/>
        <composite:attribute name="obj_name" required="true"/>
        <composite:attribute name="fonction_action" required="true"/>        
        <composite:attribute name="fonction_retour" default="goBack"/>        
    </composite:interface>

    <composite:implementation>
        <br/><br/>
        <h:outputLabel value="#{cc.attrs.message}"/>&nbsp;<h:outputText style="font-weight: bold;" value="[#{cc.attrs.obj_name}]"/>?
        <br/><br/><br/>
        <b:commandButton look="primary" type="submit" value="#{msg.BNT_OUI}" action="#{cc.attrs.bean[cc.attrs.fonction_action]}"/>
        <b:commandButton  look="primary" type="submit" value="#{msg.BNT_NON}" action="#{cc.attrs.bean[cc.attrs.fonction_retour]}"/>
    </composite:implementation>
</html>